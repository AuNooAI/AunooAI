{
  "providers": [
    {
      "name": "newsapi",
      "display_name": "NewsAPI",
      "config_type": "api_key",
      "description": "Access news articles from various sources worldwide",
      "enabled": true
    },
    {
      "name": "thenewsapi",
      "display_name": "TheNewsAPI",
      "config_type": "api_key",
      "description": "Access news articles from TheNewsAPI service",
      "enabled": true
    },
    {
      "name": "firecrawl",
      "display_name": "Firecrawl Scraper",
      "config_type": "api_key",
      "description": "Scrape websites for news articles",
      "enabled": true
    },
    {
      "name": "arxiv",
      "display_name": "ArXiv",
      "config_type": "none",
      "description": "Access scientific papers and research articles",
      "enabled": true
    },
    {
      "name": "bluesky",
      "display_name": "Bluesky",
      "config_type": "api_key",
      "description": "Access posts from the Bluesky social network",
      "enabled": true,
      "api_key_required": true,
      "api_key_name": "PROVIDER_BLUESKY_USERNAME",
      "api_key_description": "Your Bluesky username and password",
      "api_key_placeholder": "Enter your Bluesky handle",
      "api_key_help": "Your Bluesky handle (e.g. example.bsky.social or email)",
      "extra_fields": [
        {
          "field": "PROVIDER_BLUESKY_PASSWORD",
          "label": "Password",
          "placeholder": "Enter your Bluesky password",
          "type": "password"
        }
      ]
    },
    {
      "name": "elevenlabs",
      "display_name": "ElevenLabs",
      "config_type": "api_key",
      "enabled": true,
      "description": "AI Voice Generation and Podcast Creation",
      "api_key_required": true,
      "api_key_name": "ELEVENLABS_API_KEY",
      "api_key_description": "Your ElevenLabs API key",
      "api_key_placeholder": "Enter your ElevenLabs API key",
      "api_key_validation": "^sk_[a-zA-Z0-9]{32,}$",
      "api_key_validation_message": "Invalid ElevenLabs API key format",
      "api_key_help": "Get your API key from https://elevenlabs.io/account/api-key"
    },
    {
      "name": "dia",
      "display_name": "Dia TTS",
      "config_type": "api_key",
      "description": "Open-weights Text-to-Speech (dialogue) service",
      "enabled": true,
      "api_key_required": true,
      "api_key_name": "DIA_API_KEY",
      "api_key_placeholder": "Enter Dia API key",
      "api_key_help": "See https://github.com/nari-labs/dia for setup",
      "extra_fields": [
        {
          "field": "DIA_TTS_URL",
          "label": "Dia Base URL",
          "placeholder": "http://localhost:8000/v1/tts",
          "type": "text"
        }
      ]
    }
  ]
} 